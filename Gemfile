source "https://rubygems.org"

# Make sure Jekyll 3.3 is running
gem "github-pages", group: :jekyll_plugins

# The plugins for the site
group :jekyll_plugins do
    gem 'jekyll-feed'
    gem "jekyll-sitemap"
    gem "jekyll-mentions"
    gem "jekyll-paginate"
    gem "jekyll-seo-tag"
    gem "jekyll-redirect-from"
    gem "jemoji"
end
